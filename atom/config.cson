"*":
  "autocomplete-plus": {}
  autosave:
    enabled: true
  core:
    disabledPackages: [
      "racer"
    ]
    projectHome: "~/Sites/"
    telemetryConsent: "no"
  editor:
    backUpBeforeSaving: true
    invisibles:
      cr: "↩"
      eol: "↵"
      tab: "⟶"
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrapAtPreferredLineLength: true
    tabLength: 2
  "exception-reporting":
    userId: "2bf789d1-69e6-8abe-aec6-633f0f274810"
  "find-and-replace":
    focusEditorAfterSearch: true
    openProjectFindResultsInRightPane: true
  "git-diff":
    showIconsInEditorGutter: true
  "one-dark-ui": {}
  "status-bar": {}
  tabs:
    enableVcsColoring: true
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
